<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">

	<navigation-rule>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>inicioFan</from-outcome>
			<to-view-id>/fan.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>inicioArtista</from-outcome>
			<to-view-id>/artista.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/IngresarQAtemporal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>participarQA</from-outcome>
			<to-view-id>/ParticiparQA.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	
</faces-config>
